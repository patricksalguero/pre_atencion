.b20
	border 3px solid #ececec
	background-color white
	border-radius 30px 0px 30px 0px
	
.block-cuentas
	width 890px
	border 1px solid #ececec
	text-align center
	position relative
	.title-block-cuentas
		position absolute
		display block
		text-align center
		top -20px
		left 0
		right 0
		span 
			display inline-block
			font-size 24px
			font-family t-1
			color c-1
			background-color white
			padding 0px 10px
	.wrapper-cuentas-item
		margin-left 20px
		display inline-block
		text-align left 
		&:nth-child(3n+1)
			margin-left 0
	p
		font-size 20px
		text-align left
		margin-bottom 0
		padding-left 22px
	a
		display block
		text-decoration underline
		color c-5
		font-size 24px
		font-family t-1
	i
		font-size 20px
		display block
	


.text-italic
	font-size 19px
	text-align center
	display block
	