.g11
	.g11-title
		font-size 30px
		font-family t-1
	.g11-input
		margin 0 6px
		placeholder(#000,t-0)
		input 
			background url(../img/bg-input-68.png) top center repeat
			height 68px
			line-height 68px
			box-sizing border-box
			padding-left 20px
			font-size 20px
			font-style italic
			width 100%
			border none
			border-radius 8px
		