.g10
	border-top 1px solid c-4
	margin-top 25px
	.g10-navtab
		margin-left 305px
		position relative
		.g10-tab
			position absolute
			bottom 0
			left 0
			border 1px solid c-4
			border-bottom none
			width 185px
			height 80px
			box-sizing border-box
			padding 10px
			background-color c-7
			line-height 100%
			font-family t-1
			.btn-close
				position absolute
				top 10px
				right 10px
				background url('../img/bt-close-small.png') 0 0 no-repeat
				width 23px
				height 23px
				
				
				
			.g10-importe
				display block
				font-size 20px
				color c-5
				margin-bottom 15px
			
			.g10-retirar
				font-size 22px
			
			

.g10-cnt
	height 780px
	padding-top 40px
	background-color c-7
	box-sizing border-box

		
	.g10-main
		background-color blanco
		border 3px solid c-4
		border-radius 30px
		position relative
		

	.g10-title
		position absolute
		color c-5
		font-family t-1
		font-size 24px
		top -18px
		left 47px
		padding 0 10px
		line-height 100%
		background-color blanco
		box-shadow 0 12px 0 #f6f6f6 inset	
		font-weight normal