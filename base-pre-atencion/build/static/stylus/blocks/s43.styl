.blocktrio-promiment
	display inline-block
	vertical-align top
.blocktrio-normal
	vertical-align top
	display inline-block
.blocktrio
	article:nth-child(2)
		margin 0px 35px
.blocktrio-title
	margin 0
	font-size 24px
	font-family t-1
	color c-1
	text-align center
	margin-bottom 20px
	// c-6
.close-cuadro
	width 20px
	height 20px
	background url(../img/close-blue.png) no-repeat center center
	display block
	position absolute
	right 10px
	top 10px
.close-cuadro.center
	top 0
	bottom 0
	margin auto
.blocktrio-cuadro-prominent
	display block
	position relative
	background-color c-8
	width 278px
	height 100px
	border-bottom 1px solid white
	box-sizing border-box
	&:last-child
		border none
		border-radius 0 0 13px 13px
.text-sub-blocktrio
	font-family t-1
	font-size 22px
	text-decoration underline
	color c-6
.text-blocktrio
	margin 0
	font-size 22px
	font-family t-1
.blocktrio-wrap
	display table-cell
	vertical-align middle
	width 278px
	height 100px
	padding-left 16px
	box-sizing border-box
.blocktrio-cuadro-mini
	display table-cell
	vertical-align middle
	width 278px
	height 50px
	background c-8
	border-radius 13px
	position relative
	p
		font-size 20px
		color c-6
		padding-left 10px
		margin 0
		box-sizing border-box
.blocktrio-title-cuadro
	display table-cell
	vertical-align middle
	width 278px
	height 50px
	border 1px solid c-8
	border-radius 13px 13px 0px 0px
	box-sizing border-box
	font-size 22px
	color c-6
	text-align center
	
		
.blocktrio-solo
	display block
	background c-8
	border-radius 13px
	margin-bottom 16px
	position relative
		
.blocktrio-todos
	width 100%
	display block
	text-align center
	color c-5
	text-decoration underline
	font-family t-1
	font-size 22px
	margin-top 5px


.text-trio-item
	display table-cell
	width 333px

.number-trio
	border-top 1px solid c-8
	









